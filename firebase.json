{
  "hosting": [
    {
      "target": "zoorush",
      "public": "dist/zoorush",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "*.[0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f][0-9a-f].+(css|js)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public,max-age=31536000,immutable"
            }
          ]
        }
      ],
      "cleanUrls": true,
      "trailingSlash": false,
      "appAssociation": "AUTO"
    }
  ],
  "emulators": {
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    }
  }
}