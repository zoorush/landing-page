    "ng-chartist": "4.1.0",
    "npm": "^7.24.1",
    "rxfire": "^6.0.0",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.0",
    "sass": "^1.29.0",
    "tslib": "^2.3.0",
