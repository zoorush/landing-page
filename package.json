{
  "name": "zoorush",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm-run-all -p lint:style:css lint:style:scss lint:code -s format:check",
    "lint:style:css": "stylelint --fix **/*.css --allow-empty-input",
    "lint:style:scss": "stylelint --syntax=scss --fix **/*.scss --allow-empty-input",
    "lint:code": "eslint --ext .js,.ts,.jsx,.tsx --fix .",
    "format:check": "prettier --check --ignore-unknown .",
    "format:write": "prettier --write --ignore-unknown .",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.0",
    "@angular/cdk": "12.2.7",
    "@angular/common": "^12.0.0",
    "@angular/compiler": "^12.0.0",
    "@angular/core": "^12.0.0",
    "@angular/forms": "^12.0.0",
    "@angular/material": "12.2.7",
    "@angular/platform-browser": "^12.0.0",
    "@angular/platform-browser-dynamic": "^12.0.0",
    "@angular/router": "^12.0.0",
    "firebase": "^9.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "^12.0.0",
    "@types/jasmine": "~3.9.0",
    "@types/node": "^12.11.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "~4.3.5"
  }
}